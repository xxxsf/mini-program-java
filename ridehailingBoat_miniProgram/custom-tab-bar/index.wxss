.tab-bar {
  position: fixed;
  left: 0; right: 0; bottom: 0;
  height: 52px;
  display: flex;
  background: #fff;
  border-top: 1px solid #eee;
}
.tab-item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.icon {
  width: 20px;
  height: 20px;
}
.label {
  font-size: 12px;
  margin-top: 2px;
  color: #333;
}